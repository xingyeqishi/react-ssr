{
  "apps": [
    {
      "name": "react-ssr",
      "script": "src/app.js",
      "env": {
        "NODE_ENV": "PRODUCTION"
      },
      "exec-interpreter": "./node_modules/@babel/node/bin/babel-node.js",
      "node_args": "",
      "exec_mode": "cluster_mode",
      "instances": 0,
      "log_date_format": "YYYY_MM_DD HH:mm Z",
      "max_memory_restart": "700M",
      "merge_logs": true,
      "error_file": "/var/xingyeqishi/logs/react-ssr/error.log",
      "out_file": "/var/xingyeqishi/logs/react-ssr/out.log",
      "pid_file": "/var/xingyeqishi/logs/react-ssr/react-ssr.pid",
      "watch": false
    }
  ]
}
